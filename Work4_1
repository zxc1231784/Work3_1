
public class Work4_1 {

	public static void main(String[] args) {
		int sum = 0;
		for (int i = 1; i <= 10; i++) {
			int x = (int) (Math.random() * 100) + 1;
			System.out.print(x + " , ");
			sum = sum + x;
			
		}
		System.out.println();
		System.out.println(sum);
		System.out.println(sum / 10);

	}

}
