
public class Work5_2 {

	public static void main(String[] args) {
		int a[] = new int[6];
		t1: for (int i = 0; i < a.length; i++) {
			a[i] = (int) (Math.random() * 42) + 1;
			for (int j = 0; j < i; j++) {
				if (a[i] == a[j]) {
					i--;
					continue t1;
				}

			}
			System.out.println(a[i]);

		}

	}
}
