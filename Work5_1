
public class Work5_1 {

	public static void main(String[] args) {
		double a[] = { 84, 0, 94, 79.9, 90.5, 82.4, 65.7, 72.8, 99.0, 85.2, 100 };
		double sum = 0;
		double avg = 0;
		for (int i = 0; i < a.length; i++) {
			sum = sum + a[i];

		}
		avg = sum / a.length;
		System.out.println("陣列的平均數為:" + avg);
		for (int i = 0; i < a.length; i++) {
			if (a[i] >= avg) {
				System.out.print(a[i] + " , ");
			}
		}

	}

}
